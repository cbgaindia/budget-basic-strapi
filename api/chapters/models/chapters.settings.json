{
  "kind": "collectionType",
  "collectionName": "chapters",
  "info": {
    "name": "Chapter",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Chapter_No": {
      "type": "integer"
    },
    "sections": {
      "collection": "sections",
      "via": "chapter"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "icon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "Desc": {
      "type": "text",
      "maxLength": 100
    },
    "tooltips": {
      "collection": "tooltips"
    },
    "chapter_head_image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "book": {
      "via": "chapters",
      "model": "book"
    }
  }
}
